            <param-name>configFile</param-name>
            <!--            <param-value>C:/research/GamifiedRelJudgements/init.properties</param-value>-->
            <param-value>/Users/lizeyuan/Google/prac/IR/GamifiedRelJudgements/init.properties</param-value>
